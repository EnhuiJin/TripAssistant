<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tripassistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tripassistant.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tripassistant.EntertainmentSelectActivity"
            android:label="@string/entertainment"
            android:parentActivityName="com.example.tripassistant.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
        <activity
            android:name="com.example.tripassistant.AttractionSelectActivity"
            android:label="@string/attraction"
            android:parentActivityName="com.example.tripassistant.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
        <activity
            android:name="com.example.tripassistant.RestaurantSelectActivity"
            android:label="@string/restaurant"
            android:parentActivityName="com.example.tripassistant.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
        <activity
            android:name="com.example.tripassistant.SettingActivity"
            android:label="@string/title_activity_setting"
            android:parentActivityName="com.example.tripassistant.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
        <activity
            android:name="com.example.tripassistant.CartActivity"
            android:label="@string/title_activity_cart"
            android:parentActivityName="com.example.tripassistant.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
        <activity
            android:name="com.example.tripassistant.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name="com.example.tripassistant.AlertActivity"
            android:label="@string/title_activity_alert" >
        </activity>
        <activity
            android:name="com.example.tripassistant.AlertDialogFragment"
            android:label="@string/title_activity_alert_dialog_fragment" >
        </activity>
        <activity
            android:name="com.example.tripassistant.CheckUserDuplicateFragment"
            android:label="@string/title_activity_check_user_duplicate_fragment" >
        </activity>
        <activity
            android:name="com.example.tripassistant.HistoryActivity"
            android:label="@string/title_activity_history" 
            android:parentActivityName="com.example.tripassistant.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tripassistant.MainActivity" />
        </activity>
    </application>

</manifest>
